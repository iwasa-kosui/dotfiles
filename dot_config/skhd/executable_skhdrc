shift + ctrl - a : yabai -m window --focus west
shift + ctrl - d : yabai -m window --focus east

shift + ctrl - q : yabai -m window --toggle float --grid 12:12:0:0:3:12
shift + ctrl - w : yabai -m window --toggle float --grid 12:12:3:0:6:12
shift + ctrl - e : yabai -m window --toggle float --grid 12:12:9:0:3:12

shift + ctrl - z : yabai -m window --swap west 
shift + ctrl - c : yabai -m window --swap east
